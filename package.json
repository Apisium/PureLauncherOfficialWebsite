{
  "name": "pure-launcher-official-website",
  "version": "0.0.0",
  "description": "The official website for PureLauncher.",
  "scripts": {
    "clean": "del-cli dist",
    "start": "parcel index.html",
    "build": "npm run clean && parcel build index.html --public-url . --no-source-maps",
    "test": "eslint src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apisium/PureLauncherOfficialWebsite.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "keywords": [
    "PureLauncher",
    "Minecraft",
    "Forge",
    "LiteLoader",
    "Fabric",
    "Optifine"
  ],
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apisium/PureLauncherOfficialWebsite/issues"
  },
  "homepage": "https://github.com/Apisium/PureLauncherOfficialWebsite#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5"
  },
  "dependencies": {
    "@types/jquery": "^3.3.35",
    "@types/node": "^13.13.1",
    "copy-to-clipboard": "^3.3.1",
    "del-cli": "^3.0.0",
    "less": "^3.11.1",
    "parcel": "^1.12.4",
    "pure-launcher": "git+https://github.com/Apisium/PureLauncher.git",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
